<!DOCTYPE html>
<html>
  <head>
    <title>Swetha</title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

<body>

<div id="upbg"></div>

<div id="outer">

  <!-- header -->
  <%= render :partial => "contact/header" %>


  <!-- headerpic -->
  <%= render partial: "contact/headerpic" %>
  
  <!-- menu -->
  <%= render partial: "contact/menu" %>
  
  <!-- content -->
  <%= render :partial => "contact/content" %>
  
    <div class="divider1"></div>


    <!-- Primary content: Stuff that goes in the primary content column (by default, the left column) -->
    <div id="primarycontainer">
      <div id="primarycontent">
        <!-- Primary content area start -->
    
        <div class="post">
          <%= yield %>
        </div>
    
        <!-- divider -->
        <%= render partial: "contact/divider" %>
        <!-- divider2 -->
        <%= render :partial => "contact/divider2" %>
        <!-- Primary content area end -->
      </div>
    </div>

    
    <!-- Secondary content: Stuff that goes in the secondary content column (by default, the narrower right column) -->
    <div id="secondarycontent">
      <!-- Secondary content area start -->
      
      <!-- HINT: Set any div's class to "box" to encapsulate it in (you guessed it) a box -->
      <!-- secondary content -->
      <%= render :partial => "contact/sec-content" %>
      <!-- Secondary content area end -->
    </div>


  </div>

  <!-- footer -->
  <%= render partial: "contact/footer" %>
</div>

</body>
</html>
